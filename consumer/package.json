{
  "name": "connect-consumer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "dev": "better-npm-run dev",
    "start": "better-npm-run start",
    "test": "better-npm-run test",
    "coverage": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- test/*.spec.js"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node src/worker",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node src/worker",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "mocha --compilers js:babel-register test/*.spec.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.4.2",
    "aws-sdk": "^2.4.14",
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.10",
    "config": "^1.21.0",
    "debug": "^2.2.0",
    "joi": "^9.0.4",
    "jsonwebtoken": "^7.1.7",
    "lodash": "^4.14.2",
    "superagent": "^2.1.0",
    "superagent-promise": "^1.1.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.1",
    "babel-istanbul": "^0.11.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.10.2",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "nodemon": "^1.8.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  }
}
